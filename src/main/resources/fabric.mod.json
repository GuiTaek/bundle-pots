{
	"schemaVersion": 1,
	"id": "bundle_pots",
	"version": "${version}",
	"name": "bundle pots",
	"description": "The pots are usable as an replacement for bundles, that means, they can contain up to one stack worth of items, an arbitrary number of different items. When you break them so they don't shatter, they retain their content.",
	"authors": [
		"GuiTaek"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/bundle-pots/versions",
		"sources": "https://github.com/GuiTaek/bundle-pots"
	},
	"license": "MIT",
	"icon": "assets/bundlepots/images/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"guitaek.bundlepots.BundlePots"
		],
		"fabric-datagen": [
			"guitaek.bundlepots.BundlePotsDataGenerator"
		]
	},
	"mixins": [
		"bundlepots.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.24",
		"minecraft": "~1.20.3-alpha.23.44.a",
		"java": ">=17",
		"fabric-api": "*"
	}
}